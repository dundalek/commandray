{
  "name": "pg:push",
  "usage": "heroku pg:push SOURCE TARGET",
  "desc": "push from SOURCE_DATABASE to REMOTE_TARGET_DATABASE",
  "params": [
    {
      "name": "-a, --app APP",
      "desc": "app to run command against"
    },
    {
      "name": "-r, --remote REMOTE",
      "desc": "git remote of app to run command against"
    }
  ],
  "subcommands": [],
  "examples": [
    "heroku pg:push mylocaldb postgresql-swimmingly-100",
    "heroku pg:push postgres://myhost/mydb postgresql-swimmingly-100"
  ]
}