{
  "name": "pg:pull",
  "usage": "heroku pg:pull <REMOTE_SOURCE_DATABASE> <TARGET_DATABASE>",
  "desc": "pull from REMOTE_SOURCE_DATABASE to TARGET_DATABASE",
  "params": [
    {
      "name": "-a, --app APP",
      "desc": "app to run command against"
    },
    {
      "name": "-r, --remote REMOTE",
      "desc": "git remote of app to run command against"
    }
  ],
  "subcommands": [],
  "examples": [
    "heroku pg:push postgresql-swimmingly-100 mylocaldb",
    "heroku pg:push postgresql-swimmingly-100 postgres://myhost/mydb"
  ]
}